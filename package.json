{
  "name": "special-person-project",
  "version": "1.0.0",
  "description": "This is about Guido van Rossum.",
  "keywords": [],
  "homepage": "https://codesandbox.io/s/new",
  "main": "src/index.js",
  "dependencies": {
    "babel-loader": "7.1.4",
    "concurrently": "3.5.1",
    "material-ui": "1.0.0-beta.40",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.0"
  },
  "scripts": {
    "start":
      "npm run build:env && \"./node_modules/.bin/concurrently\" -p \"[{name}]\" -n \"LAMBDA,REACT\" \"npm run start:lambda\" \"npm run start:react\"",
    "start:react": "./node_modules/.bin/react-scripts start",
    "start:lambda":
      "./node_modules/.bin/netlify-lambda serve src/lambda/routes",
    "build":
      "npm run build:env && \"./node_modules/.bin/concurrently\" -p \"[{name}]\" -n \"LAMBDA,REACT\" \"npm run build:lambda\" \"npm run build:react\"",
    "build:env": "exit 0",
    "build:react": "./node_modules/.bin/react-scripts build",
    "build:lambda":
      "./node_modules/.bin/netlify-lambda build src/lambda/routes",
    "test": "./node_modules/.bin/react-scripts test --env=jsdom"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0"
  }
}
